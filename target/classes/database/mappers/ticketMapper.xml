<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lg.t2.ticket.TicketDAO">
	<select id="getList"  resultType="com.lg.t2.ticket.TicketDTO">
		select * from lgtwins order by playdate asc
	</select>
	
	<select id="getSelect" parameterType="com.lg.t2.ticket.TicketDTO" resultType="com.lg.t2.ticket.TicketDTO">
		select * from lgtwins where num=#{num}
	</select>
	
	<insert id="setWrite" parameterType="com.lg.t2.ticket.TicketDTO">
		insert into ticket values(ticket_SEQ.NEXTVAL,#{teamName},#{teamLogo},#{playDate},#{sitNum},#{price})
	</insert>
</mapper>  