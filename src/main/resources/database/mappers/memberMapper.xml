<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.lg.t2.member.MemberDAO">

	<select id="memberJoin" parameterType="com.lg.t2.member.MemberDTO)">
		insert into member values(#{id}, #{pw}, #{name}, #{phone} )
	</select>

	<select id="memberLogin" parameterType="com.lg.t2.member.MemberDTO" resultType="com.lg.t2.member.MemberDTO">
	
		select * from member where id=#{id} and pw=#{pw}
	</select>






</mapper>